<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="org.example.entidadesPersistencia.EstudianteEntity">
        <table name="estudiante" schema="esquema_estudiantes" catalog=""/>
        <attributes>
            <id name="idEstudiante">
                <column name="idEstudiante" precision="-1"/>
            </id>
            <basic name="cedula">
                <column name="Cedula" length="45"/>
            </basic>
            <basic name="nombres">
                <column name="Nombres" length="45"/>
            </basic>
            <basic name="apellidos">
                <column name="Apellidos" length="45"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
