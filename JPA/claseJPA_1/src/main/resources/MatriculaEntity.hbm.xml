<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.example.entidades.MatriculaEntity" table="matricula" schema="esquema_estudiantes">
        <id name="idMat">
            <column name="idMat" sql-type="int"/>
        </id>
        <property name="carrera">
            <column name="Carrera" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="ciclo">
            <column name="Ciclo" sql-type="int" not-null="true"/>
        </property>
        <property name="anio">
            <column name="Anio" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="estudianteIdEstudiante">
            <column name="Estudiante_idEstudiante" sql-type="int"/>
        </property>
        <many-to-one name="estudianteByEstudianteIdEstudiante" class="org.example.entidades.EstudianteEntity" insert="false" update="false">
            <column name="Estudiante_idEstudiante"/>
        </many-to-one>
    </class>
</hibernate-mapping>